<!--form表单-->
<!--x轴滚动 显示当前车次路线-->
<view class='wrapX'>
	<view class="wrapOut">
		<view class="wrapInner">
			<view class="contant">本车路线：{{notice}}</view>
		</view>
	</view>
</view>

<!--地图容器-->
<!--longitude及latitude为设置为调转到指定地址位置，默认不显示-->
<map id="myMap"
    style="width:100%;height:500rpx;"
    longitude="{{longitude}}"
    latitude="{{latitude}}"
    polyline="{{polyline}}"
    markers="{{markers}}"
    scale='11' 
    show-location>
</map>

<view class="pattern">本次车辆为:{{bus}}</view>

<view class="pattern">可乘坐的人数为：{{people}}</view>

